{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Http Mcp Client Tests",
      "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
      "args": [
        "${workspaceFolder}/src/utils/mcp/index.http.test.ts",
        "--run"
      ],
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "WebSocket Mcp Client Tests",
      "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
      "args": [
        "${workspaceFolder}/src/utils/mcp/index.websocket.test.ts",
        "--run"
      ],
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "WebSocket KeepAlive Tests",
      "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
      "args": [
        "${workspaceFolder}/src/utils/mcp/websocket.test.ts",
        "--run"
      ],
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "WebSocket Authorization Tests",
      "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
      "args": [
        "${workspaceFolder}/src/utils/mcp/authorization.test.ts",
        "--run"
      ],
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
      "console": "integratedTerminal"
    }
  ]
}
